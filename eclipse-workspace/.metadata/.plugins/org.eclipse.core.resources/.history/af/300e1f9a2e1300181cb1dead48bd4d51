package arraylist.maison;

public class MyArrayList {
	
	private Object[] list;
	
	public MyArrayList() {
		
	}
	

	public void add(Object obj) {
		
		if (list==null)
			list=new Object[1];
			
		Object [] listTemp=new Object[list.length+1];
		
		for (int i=0;i<list.length;i++) {
			listTemp[i]=list[i];
		}
		
		listTemp[list.length+1]=obj;
		
	}
	
	
	
}
