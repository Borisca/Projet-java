package arraylist.maison;

public class Main {

	public static void main(String[] args) {

		MyArrayList list=new MyArrayList();
		
		
		list.add("Bob");
		list.add("Bill");
		list.add("Joe");
		list.add("Bob");
		list.add("Bill");
		list.add("Joe");
		
		list.remove("Bill");
		
		System.out.println(list.get(4));
		System.out.println(list.size());
		
	}

}
